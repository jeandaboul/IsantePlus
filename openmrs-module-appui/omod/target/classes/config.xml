<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>appui</id>
	<name>App UI Module</name>
	<version>1.6-SNAPSHOT</version>
	<package>org.openmrs.module.appui</package>
	<author>OpenMRS</author>
	<description>
		Support for using the App Framework's context in the UI Framework
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/appui/update.rdf</updateURL>
	<require_version>1.9.1</require_version>
	<!-- / Module Properties -->

	<!-- Required Modules -->
	<require_modules>
		<require_module version="2.0">
			org.openmrs.module.appframework
		</require_module>
		<require_module version="2.5.2">
			org.openmrs.module.uiframework
		</require_module>
        <require_module version="${uicommonsModuleVersion}">
            org.openmrs.module.uicommons
        </require_module>
        <require_module version="${webservicesRestVersion}">
            org.openmrs.module.webservices.rest
        </require_module>
	</require_modules>
	<!-- / Required Modules -->
	
	<!-- Module Activator -->
	<activator>org.openmrs.module.appui.AppUiActivator</activator>

	<!-- Maps hibernate files, if present -->
	<mappingFiles>
	</mappingFiles>

	<!-- Internationalization -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

