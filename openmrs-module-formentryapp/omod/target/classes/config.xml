<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>formentryapp</id>
	<name>Form Entry App Module</name>
	<version>1.3-SNAPSHOT</version>
	<package>org.openmrs.module.formentryapp</package>
	<author>OpenMRS</author>
	<description>
		Adds forms to OpenMRS 2.x
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/formentryapp/update.rdf</updateURL>
	<require_version>1.9.7</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.formentryapp.FormEntryAppActivator</activator>
	
	<!-- Required Modules -->
	<require_modules>
		<require_module version="2.2.1">
			org.openmrs.module.appframework
		</require_module>
		<require_module version="3.2">
			org.openmrs.module.uiframework
		</require_module>
	</require_modules>
	<!-- / Required Modules -->
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with formentryapp. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

