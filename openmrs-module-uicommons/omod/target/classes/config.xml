<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>uicommons</id>
	<name>UI Commons Module</name>
	<version>1.9-SNAPSHOT</version>
	<package>org.openmrs.module.uicommons</package>
	<author>OpenMRS</author>
	<description>
		UI Common Library that has common JS/CSS/HTML/templates/fragments etc.
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/uicommons/update.rdf</updateURL>
    <require_version>1.9.3</require_version>
    <!-- / Module Properties -->

    <require_modules>
        <require_module version="${uiframeworkVersion}">
            org.openmrs.module.uiframework
        </require_module>
    </require_modules>

    <!-- Module Activator -->
	<activator>org.openmrs.module.uicommons.UicommonsActivator</activator>
	
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with uicommons. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>ht</lang>
		<file>messages_ht.properties</file>
	</messages>
	<messages>
		<lang>pt</lang>
		<file>messages_pt.properties</file>
	</messages>
	<messages>
		<lang>de</lang>
		<file>messages_de.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

